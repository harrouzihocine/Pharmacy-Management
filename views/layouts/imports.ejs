<script>
 $(document).ready(function () {
  $("#search").on("keyup", function () {
    var value = $(this).val().toLowerCase();
    $("#tablebody tr").filter(function () {
      // Filter rows by matching text from all cells except the last one
      var match = $(this)
        .find("td:not(:last-child)")
        .toArray()
        .some(function (cell) {
          return $(cell).text().toLowerCase().indexOf(value) > -1;
        });
      $(this).toggle(match);
    });
  });
});

</script>
